  <rdf:Description rdf:about="http://dbpedia.org/resource/Pet">
    <rdfs:label xml:lang="ar">حيوان منزلي</rdfs:label>
    <prov:wasDerivedFrom rdf:resource="http://en.wikipedia.org/wiki/Pet?oldid=683449223"/>
    <owl:sameAs rdf:resource="http://www.wikidata.org/entity/Q39201"/>
    <dbo:wikiPageExternalLink rdf:resource="http://www.hsus.org/web-files/PDF/hsp/soa_ii_chap02.pdf"/>
