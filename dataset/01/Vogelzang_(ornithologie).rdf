  <rdf:Description rdf:about="http://nl.dbpedia.org/resource/Vogelzang_(ornithologie)">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Vogelzang (ornithologie)</rdfs:label>
    <dbpedia-owl:wikiPageLength rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">4722</dbpedia-owl:wikiPageLength>
    <foaf:isPrimaryTopicOf rdf:resource="http://nl.wikipedia.org/wiki/Vogelzang_(ornithologie)"/>
