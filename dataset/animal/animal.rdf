  <rdf:Description rdf:about="http://purl.org/ontology/wo/populationTrend">
    <rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/wo/"/>
    <rdfs:domain rdf:resource="http://purl.org/ontology/wo/ConservationStatus"/>
